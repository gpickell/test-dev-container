{
    "name": "VertiGIS Studio for Codespaces",    
    "dockerComposeFile": "docker-compose.yml",
    "service": "code",
    "customizations": {
        "codespaces": {
            "openFiles": ["README.md"]
        }
    },
    "hostRequirements": {
        "cpus": 4
    },
    "forwardPorts": [8080],
    "portsAttributes": {
        "3000": {
            "label": "Studio",
            "onAutoForward": "openBrowser"                        
        }
    }
}
